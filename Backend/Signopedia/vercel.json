{
    "version":2,
    "builds":[
        {
            "src":"Backend/Signopedia/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxLambdaSize":"1024mb","runtine":"python3.9"}
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "config":{
                "distDir":"staticfiles_build"
            }
        }
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },{
            "src":"/(.*)",
            "dest":"Backend/Signopedia/wsgi.py"
        }
    ]
}